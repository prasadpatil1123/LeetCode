// https://leetcode.com/problems/reverse-string/description/
class Solution {
    public void reverseString(char[] s) {
        char[] xyz= new char[s.length];
        for(int x = 0; x < s.length; x++){
            xyz[x] = s[s.length-x-1];
        }
        for(int x = 0; x < s.length; x++){
            s[x] = xyz[x];
        }
    }
}
